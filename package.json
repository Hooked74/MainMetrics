{
  "name": "MainMetrics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel public/index.html -p 3000",
    "build": "rimraf dist && parcel build public/index.html --public-url '.'",
    "test": "node scripts/test.js --env=jsdom",
    "test-staged": "cross-env CI=true node scripts/test.js --env=jsdom --bail --findRelatedTests",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "prettier": "prettier --write",
    "lint": "tslint --fix -c ./tslint-base.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hooked74/MainMetrics.git"
  },
  "engines": {
    "node": ">= 9.11.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hooked74/MainMetrics/issues"
  },
  "homepage": "https://github.com/Hooked74/MainMetrics#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/src/polyfills.js",
      "jest-canvas-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".mock",
      "index.d"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".mock",
      "index.d"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/lib"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "yarn lint",
      "yarn test-staged",
      "git add"
    ],
    "*.{js,jsx,json,scss,css,md}": [
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.2",
    "@types/react": "^16.4.12",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^9.1.3",
    "class-names": "^1.0.0",
    "classlist-polyfill": "^1.2.0",
    "components": "^0.1.0",
    "containers": "^0.0.1",
    "core-js": "^2.5.7",
    "cross-env": "^5.2.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-canvas-mock": "^1.1.0",
    "less": "^3.8.1",
    "lint-staged": "^7.2.2",
    "modules": "^0.4.0",
    "node-sass": "^4.9.3",
    "normalize-scss": "^7.0.1",
    "parcel-bundler": "^1.9.7",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-modules": "^1.3.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "store": "^2.0.12",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1",
    "url-polyfill": "^1.0.14",
    "utils": "^0.3.1",
    "whatwg-fetch": "^2.0.4",
    "yargs": "^12.0.1"
  }
}
